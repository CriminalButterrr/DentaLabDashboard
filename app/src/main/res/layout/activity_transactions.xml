                android:layout_gravity="center"
                android:layout_weight="1"
                android:padding="5dp"
                android:text="10 Hexgel" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:padding="5dp"
